

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model Location {
  id          Int      @id @default(autoincrement())
  name        String
  description String?  
  latitude    Float
  longitude   Float
  category    String   @default("umum")
  createdAt   DateTime @default(now())
}